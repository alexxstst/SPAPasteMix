
<div class="container">
    <div class="row">
        <div class="col-md-2"></div>
        <div class="col-md-8">
            <ul class="list-group">
                <li ng-repeat="p in posts">
                    <a href="#!read/{{p.uuid}}" class="list-group-item">
                        {{ p.title }} | {{ p.create }} | {{ p.expired }}
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>
